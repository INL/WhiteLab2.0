<div class="row">
	<div class="col-xs-12">
		<div id="results" class="panel panel-default">
			<div class="panel-heading">
				<label><%= t(:"page_titles.keys.results").capitalize %></label>
			</div>
			<div class="panel-body">
				<div id="result-pane" class="row" data-query-id="<%= query.id %>" data-query-status="<%= query.status %>" data-query-page="<%= query.page %>" data-query-patt="<%= query.patt %>" data-query-filter="<%= query.filter %>" data-query-within="<%= query.within %>" data-query-view="<%= query.view %>">
					<% if query.finished? %>
						<% if @namespace %>
						<%= render partial: "/#{@namespace}/queries/result", :locals => { :query => query } %>
						<% else %>
						<%= render partial: "/queries/result", :locals => { :query => query } %>
						<% end %>
					<% elsif query.failed? %>
					<p>Failed to execute query.</p>
					<% else %>
					<span class="loading"></span>
					<% end %>
				</div>
			</div>
		</div>
	</div>
</div>
