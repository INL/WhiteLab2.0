<a id="history-toggle" href="#" class="show-history btn btn-info <%= 'hidden' unless queries.any? || export_queries.any? %>" data-query-id="<%= query.id if query %>"><span class="glyphicon glyphicon-time"></span></a>
<% if query %>
	<%= render partial: '/queries/details', :locals => { :query => query } %>
	<%= render partial: '/queries/results', :locals => { :query => query } %>
<% end %>
