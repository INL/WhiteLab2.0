$('#query div#query-details').html("<%= escape_javascript(raw render partial: "/query/details", layout: false) %>");
<% if !@query.query_result.is_finished && (@query.query_result.view < 8 || !@query.query_result.group.blank? ) %>
setTimeout(function(){
