$('#history-dialog').dialog('option', 'title', '<%= t(:'queries.keys.query_history').capitalize %>');
$('#query-list').html("<%= escape_javascript(raw render partial: "/query/history", layout: false) %>");
var queryId = $("a.show-history").data('query-id');
if (queryId != null) {
	$("tr#"+queryId).addClass("active");
	$("tr#"+queryId+" a.remove-query").addClass("hidden");
}

