<% fields = [] %>
<% fields << { key: "label", header: t(:'data_labels.keys.label').capitalize, sortable: true, method_name: "label" } %>
<% # fields << { key: "token_count", header: t(:'data_labels.keys.frequency').capitalize, sortable: true, method_name: "token_count" } %>
<% corpora.each do |corpus| %>
	<% fields << { key: "token_count_#{corpus}", header: t(:'data_labels.keys.frequency').capitalize+" "+corpus, sortable: true, method_name: "token_count_#{corpus}" } %>
<% end %>
<%= render partial: '/layouts/data_table', layout: false, locals: { fields: fields, url: '/admin/index?tab=posheads', things: posheads, model: 'pos-head' } %>