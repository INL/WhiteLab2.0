<% provide(:title, t(:'page_titles.keys.settings').capitalize+" "+t(:'page_titles.keys.overview').capitalize) %>
<div class="row margin-top-20">
	<div class="col-xs-12 col-sm-4 col-lg-3">
		<ul class="nav nav-pills nav-stacked">
			<% if !@tab || @tab.eql?('cql') %>
				<li class="active"><a href="#" disabled>CQL Tester</a></li>
			<% else %>
				<li><%= link_to 'CQL Tester', admin_page_url(:page => 'overview', :tab => 'cql') %></li>
			<% end %>
			<% if @tab && @tab.eql?('qbm') %>
				<li class="active"><a href="#" disabled>Query Benchmarker</a></li>
			<% else %>
				<li><%= link_to 'Query Benchmarker', admin_page_url(:page => 'overview', :tab => 'qbm') %></li>
			<% end %>
		</ul>
	</div>
	<div class="col-xs-12 col-sm-8 col-lg-9">
		<% if !@tab || @tab.eql?('cql') %>
			<%= render partial: '/admin/overview/cql', layout: false %>
		<% elsif @tab.eql?('qbm') %>
			<%= render partial: '/admin/overview/benchmark', layout: false %>
		<% end %>
	</div>
</div>
